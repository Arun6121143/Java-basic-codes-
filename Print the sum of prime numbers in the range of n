import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		int n=scn.nextInt();
		int count,sum=0;
		for(int i=2;i<=n;i++){
		    count=0;
		    for(int k=1;k<=i;k++){
		          if(i%k==0){
		            count++;
		             }
		    }
		        if(count==2){
		            sum=sum+i;
		             }
		    }
		System.out.println(sum);
		}
}
